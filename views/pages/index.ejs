      console.log(response);
      alert(response);
    };
    var handleError = function(response) {
      alert("This is error");      
        alert(response);
      // handle missing API errors here (missing scopes, back shopifyql, etc...)
    };
    ShopifyApp.Analytics.fetch({
      query: shopifyQL,
      success: renderData,
      error: handleError
    });
  });
</script>